{
	"version": "0.2.0",
	"configurations": [
		{
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
	  	"smartStep": false,
      "program": "${workspaceFolder}\\app.js"
    },
		{
			"type": "node",
			"request": "attach",
			"name": "ES6 Debugger",
			"protocol": "inspector",
			"restart": true,
			"processId": "${command:PickProcess}",
		}
	]
}